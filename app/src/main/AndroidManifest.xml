<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.christ.android_mvvm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.ShowActivity"></activity>
        <activity android:name=".view.ShowFamousSayActivity" />
        <activity android:name=".view.ShowFamousActivity" />
        <activity android:name=".view.FamousSayActivity" />
        <activity android:name=".view.DemoActivity" />
        <activity android:name=".view.FruitActivity" />
        <activity android:name=".view.FamousActivity" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity android:name=".view.SpringBootActivity" />
        <activity android:name=".view.GetRequestActivity" />
        <activity android:name=".view.PostRequestActivity" />
        <activity android:name=".view.TestDataBindingActivity" />
        <activity android:name=".view.WebViewActivity" />
        <activity android:name=".view.HomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>